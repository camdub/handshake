{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "User",
  "description": "A basic User object",
  "type": "object",
  "properties": {
    "user_name" : {
      "type" : "string"
    },
    "first_name" : {
      "type" : "string"
    },
    "last_name" : {
      "type" : "string"
    },
    "email" : {
      "type" : "string"
    },
    "phone_number" : {
      "type" : "string"
    },
    "handshake_access_token" : {
      "type" : "string"
    }
  },
  "required" : ["user_name","first_name","last_name","email","phone_number","handshake_access_token"]
}
